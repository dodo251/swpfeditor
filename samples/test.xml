<?xml version="1.0" encoding="utf-8"?>
<test id="T1" alias="SampleTest">
  <meta>
    <author>Test Engineer</author>
    <version>1.0</version>
    <created>2024-01-01T00:00:00Z</created>
  </meta>
  <description>Sample test demonstrating SSH connection and OS verification</description>
  <sections>
    <section id="S1" alias="Setup">
      <step id="step1" alias="GetOsViaSsh" targetType="ssh" target="DUT_SSH" timeout="30" command="uname -a">
        <extracts>
          <extract name="os" pattern="(?i)Ubuntu">
            <checks>
              <check sourceRef="os" expect="Ubuntu" />
            </checks>
          </extract>
        </extracts>
      </step>
    </section>
  </sections>
</test>
